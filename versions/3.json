{
    "home": {
        "@context": "http://localhost:9090/context",
        "id": "http://localhost:9090/home",
        "title": "Example People API Server",
        "description": "Using the people Hydra ontology",
        "people": {
            "@id": "http://localhost:9090/people"
        }
    },
    "context": {
        "@context": [
            {
                "ppl": "http://localhost:9090/onto/",
                "schema": "http://schema.org/",
                "hydra": "http://www.w3.org/ns/hydra/core#",
                "people": {
                    "@id": "ppl:people",
                    "@type": "@id"
                }
            },
            "https://www.w3.org/ns/hydra/context.jsonld"
        ]
    },
    "onto": {
        "@context": "http://localhost:9090/context",
        "@id": "http://localhost:9090/onto/",
        "@type": "owl:Ontology",
        "http://creativecommons.org/ns#attributionName": "Authors",
        "http://creativecommons.org/ns#attributionURL": {
            "@id": "http://example.com/ontology"
        },
        "http://creativecommons.org/ns#license": {
            "@id": "http://creativecommons.org/licenses/by/4.0/"
        },
        "http://purl.org/dc/terms/description": "Desc",
        "http://purl.org/dc/terms/publisher": "Authors",
        "http://purl.org/dc/terms/rights": "Copyright Â© Authors",
        "http://purl.org/vocab/vann/preferredNamespacePrefix": "ltk",
        "comment": "An example ontology for tracking daily life events. Includes Hydra API: todo maybe separate",
        "label": "The People Ontology",
        "defines": {
            "ppl:people": {
                "@type": "hydra:Link"
            }
        }
    },
    "doc": {
        "@context": "http://localhost:9090/context",
        "@id": "http://localhost:9090/doc",
        "@type": "ApiDocumentation",
        "entrypoint": "/home",
        "supportedClass": [
            {
                "@id": "schema:Person",
                "@type": "Class",
                "title": "People",
                "description": "The People class",
                "supportedOperation": [
                    {
                        "@type": "Operation",
                        "expects": "owl:Nothing",
                        "method": "GET",
                        "returns": "Collection",
                        "title": "Gets all the people"
                    }
                ],
                "supportedProperty": [
                    {
                        "@type": "SupportedProperty",
                        "title": "name",
                        "description": "The name property",
                        "property": {
                            "@id": "schema:name"
                        },
                        "readable": true,
                        "required": false,
                        "writeable": true
                    },
                    {
                        "@type": "SupportedProperty",
                        "title": "jobTitle",
                        "description": "The jobTitle property",
                        "property": {
                            "@id": "schema:jobTitle"
                        },
                        "readable": true,
                        "required": false,
                        "writeable": true
                    }
                ]
            },
            {
                "@id": "ppl:EntryPoint",
                "@type": "Class",
                "description": "The EntryPoint class",
                "supportedOperation": {
                    "@type": "Operation",
                    "description": "The entrypoint is the the API starts",
                    "expects": "owl:Nothing",
                    "method": "GET",
                    "returns": "ppl:EntryPoint",
                    "title": "Gets the API entrypoint"
                },
                "supportedProperty": [
                    {
                        "@type": "SupportedProperty",
                        "description": "The title property",
                        "property": {
                            "@id": "hydra:title",
                            "@type": "rdf:Property",
                            "range": "xsd:string"
                        },
                        "readable": true,
                        "required": false,
                        "title": "title",
                        "writeable": true
                    },
                    {
                        "@type": "SupportedProperty",
                        "description": "The description property",
                        "property": {
                            "@id": "hydra:description",
                            "@type": "rdf:Property",
                            "range": "xsd:string"
                        },
                        "readable": true,
                        "required": false,
                        "title": "description",
                        "writeable": true
                    },
                    {
                        "@type": "SupportedProperty",
                        "description": "All the people",
                        "property": {
                            "@id": "ppl:people",
                            "@type": [
                                "rdf:Property",
                                "Link"
                            ],
                            "range": "Collection",
                            "supportedOperation": {
                                "@type": "Operation",
                                "description": "",
                                "expects": "owl:Nothing",
                                "method": "GET",
                                "returns": "Collection",
                                "title": "Gets the collection of people (paged)"
                            }
                        },
                        "readable": true,
                        "required": false,
                        "title": "people",
                        "writeable": false
                    }
                ],
                "title": "EntryPoint"
            }
        ]
    }
}