                        "required": false,
                        "writeable": true
                    }
                ]
            },
            {
                "@id": "ppl:People",
                "@type": "hydra:Class",
                "subClassOf": "http://www.w3.org/ns/hydra/core#Collection",
                "label": "People",
                "description": "A collection of people",
                "supportedOperation": [
                    {
                        "@id": "ppl:CreatePerson",
                        "@type": "http://schema.org/AddAction",
                        "method": "POST",
                        "label": "Creates a new person entity",
                        "description": null,
                        "expects": "http://schema.org/Person",
                        "returns": "http://schema.org/Person",
                        "statusCodes": [
                            {
                                "code": 201,
                                "description": "If the person entity was created successfully."
                            }
                        ]
                    },
                    {
                        "@id": "ppl:GetPeople",
                        "@type": "hydra:Operation",
                        "method": "GET",
                        "label": "Retrieves all person entities",
                        "description": null,
                        "expects": null,
                        "returns": "ppl:People",
                        "statusCodes": []
                    }
                ],
                "supportedProperty": [
                    {
                        "property": "http://www.w3.org/ns/hydra/core#member",
                        "hydra:title": "members",
                        "hydra:description": "The persons",
                        "required": null,
                        "readonly": false,
                        "writeonly": false
                    }
                ]
            },
            {
                "@id": "ppl:EntryPoint",
                "@type": "Class",
                "description": "The EntryPoint class",
                "supportedOperation": {
                    "@type": "Operation",
                    "description": "The entrypoint is the the API starts",
                    "expects": "owl:Nothing",
                    "method": "GET",
                    "returns": "ppl:EntryPoint",
                    "title": "Gets the API entrypoint"
                },
                "supportedProperty": [
                    {
                        "@type": "SupportedProperty",
                        "description": "The title property",
                        "property": {
                            "@id": "hydra:title",
                            "@type": "rdf:Property",
                            "range": "xsd:string"
                        },
                        "readable": true,
                        "required": false,
                        "title": "title",
                        "writeable": true
                    },
                    {
                        "@type": "SupportedProperty",
                        "description": "The description property",
                        "property": {
                            "@id": "hydra:description",
                            "@type": "rdf:Property",
                            "range": "xsd:string"
                        },
                        "readable": true,
                        "required": false,
                        "title": "description",
                        "writeable": true
                    },
                    {
                        "@type": "SupportedProperty",
                        "description": "All the people",
                        "property": {
                            "@id": "ppl:people",
                            "@type": [
                                "rdf:Property",
                                "Link"
                            ],
                            "range": "ppl:People"
                        },
                        "readable": true,
                        "required": false,
                        "title": "people",
                        "writeable": false
                    }
                ],
                "title": "EntryPoint"
            }
        ]
    }
